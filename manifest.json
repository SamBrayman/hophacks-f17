//tells user specific things about extension
//name, description, etc.

//also provides info to chrome about what extension it's trying to run

{
  "manifest_version": 2,

  "name": "Super Cool Project Name",
  "description": "Super cool project function!",
  "version": "1.0",

  "browser_action": {
   "default_icon": "Swag.png"
  },

  "permissions": [
   "activeTab"
  ],

   "background": {
     "scripts": [
       "background.js"
     ]
   }
   ,
   "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://code.jquery.com https://cdnjs.cloudflare.com https://maxcdn.bootstrapcdn.com; object-src 'self'"
}
