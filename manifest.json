//tells user specific things about extension
//name, description, etc.

//also provides info to chrome about what extension it's trying to run

{
  "manifest_version": 2,

  "name": "Super Cool Project Name",
  "description": "Super cool project function!",
  "version": "1.0",

  "browser_action": {
   "default_icon": "Swag.png"
  },

  "permissions": [
   "activeTab"
  ],

   "background": {
     "scripts": [
       "background.js"
     ] 
   }
}
